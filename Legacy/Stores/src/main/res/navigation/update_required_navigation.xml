<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/update_required_navigation"
    app:startDestination="@id/updateRequiredActivity">

    <activity
        android:id="@+id/updateRequiredActivity"
        android:name="com.infomaniak.lib.stores.updaterequired.UpdateRequiredActivity"
        android:label="UpdateRequiredActivity"
        tools:layout="@layout/activity_update_required">
        <argument
            android:name="appId"
            app:argType="string" />
        <argument
            android:name="versionCode"
            app:argType="integer" />
        <argument
            android:name="appTheme"
            app:argType="integer" />
    </activity>
</navigation>
