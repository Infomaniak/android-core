<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bug_tracker_navigation"
    app:startDestination="@id/bugTrackerActivity">

    <activity
        android:id="@+id/bugTrackerActivity"
        android:name="com.infomaniak.lib.bugtracker.BugTrackerActivity"
        android:label="activity_bug_tracker"
        tools:layout="@layout/activity_bug_tracker">
        <argument
            android:name="user"
            app:argType="com.infomaniak.lib.core.models.user.User" />
        <argument
            android:name="appBuildNumber"
            app:argType="string" />
        <argument
            android:name="bucketIdentifier"
            app:argType="string" />
        <argument
            android:name="projectName"
            app:argType="string" />
        <argument
            android:name="repoGithub"
            app:argType="string" />
    </activity>
</navigation>
